{
  "name": "project-playwright-typescript",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "test": "npx playwright test",
    "test:headed": "npm run test --headed",
    "test:ui": "npm run test --ui",
    "open:ui-mode": "npx playwright test --ui",
    "test:debug": "npx playwright test --debug",
    "show-report": "npx playwright show-report",
    "format": "npx prettier --write .",
    "format:check": "npx prettier . --check \"!**.ts\"",
    "lint": "npx eslint . --max-warnings=0",
    "instal-project-dependencies": "npx npm i",
    "dependency:check": "npx npm-check -u",
    "pw:update": "npx npm i @playwright/test",
    "pw:update-browsers": "npx playwright install",
    "pw:check-version": "npx playwright --version",
    "pw:outdated": "npx npm outdated @playwright/test",
    "prepare": "husky"
  },
